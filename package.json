    "gatsby-transformer-yaml": "^2.4.4",
    "moment": "^2.27.0",
    "noctis-theme": "https://github.com/liviuschera/noctis.git#master",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
