{
  "name": "stefanimhoff-de",
  "private": true,
  "description": "",
  "version": "0.1.0",
  "author": "Stefan Imhoff <stefan@imhoff.name>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kogakure/website-gatsby-stefanimhoff.de"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "styled-components",
    "eslint"
  ],
  "scripts": {
    "build": "gatsby build",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "clean": "gatsby clean",
    "commit": "git-cz",
    "develop": "yarn clean & gatsby develop",
    "format": "yarn run prettier --write",
    "format:check": "yarn run prettier --list-different",
    "image:optimize": "node scripts/optimize.js",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint 'src/**/*.{ts,tsx,js,css}'",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:ts": "tsc",
    "plop": "plop",
    "prettier": "prettier --ignore-path .gitignore '**/*.{js,json,ts,tsx,md}'",
    "serve": "gatsby serve",
    "start": "run-p start:**",
    "start:gatsby": "gatsby develop -H 0.0.0.0 -o",
    "start:storybook": "start-storybook -p 9000 -c .storybook",
    "svg": "svgr --icon --filename-case kebab --template scripts/templates/svgr/icon.js --index-template scripts/templates/svgr/icon-index.js --ext tsx -d src/components/icons src/components/icons/svg",
    "test": "is-ci 'test:coverage' 'test:watch'",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:watch": "jest --watch",
    "validate": "yarn clean & npm-run-all --parallel lint:* test:coverage format:check build"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
