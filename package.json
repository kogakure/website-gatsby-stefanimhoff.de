    "format": "yarn run prettier --write",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:css": "stylelint 'src/**/*.{ts,tsx,js}'",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext md",
    "lint:ts": "tsc",
    "lint": "npm-run-all --parallel lint:*",
    "prettier": "prettier --ignore-path .gitignore '**/*.{js,json,ts,tsx,md}'",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "validate": "npm-run-all --parallel lint:* format:check build"
  },
  "dependencies": {
