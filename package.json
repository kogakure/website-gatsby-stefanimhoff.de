  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "commit": "git-cz",
    "develop": "gatsby develop",
    "format:check": "yarn run prettier --list-different",
    "format": "yarn run prettier --write",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:css": "stylelint 'src/**/*.{ts,tsx,js,css}'",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js",
    "lint:ts": "tsc",
    "lint": "npm-run-all --parallel lint:*",
    "prettier": "prettier --ignore-path .gitignore '**/*.{js,json,ts,tsx,md}'",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:watch": "jest --watch",
    "test": "is-ci 'test:coverage' 'test:watch'",
    "validate": "npm-run-all --parallel lint:* test:coverage format:check build"
  },
